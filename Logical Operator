/*
OR = ||
 AND =&&
 NOT= !
 */
 #include <iostream>
using namespace std;
int main() {
    cout << ((3 > 1) || (3 < 2)) << "\n";  // Outputs 1 (true) because 3 > 1 is true
    cout << ((3 > 1) && (3 < 2)) << "\n";  // Outputs 0 (false) because 3 < 2 is false
    cout << (3 != 3) << "\n";              // Outputs 0 (false) because 3 == 3
    return 0;
}

